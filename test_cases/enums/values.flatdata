/* This test tests that varies ways of assigning values to the different types works properly
 * MIN, MAX and 0 are assigned to test that the resulting enum can hold the full range
 * Undefined values are generated, and can be used.
 */
namespace n{
enum EnumI8 : i8 {
	FOO_I8_NEG = -128,
	FOO_I8_POS = 127,
	FOO_I8_ZERO = 0,
	FOO_I8_NEG_HEX = -0x7f,
	FOO_I8_POS_HEX = 0x7e,
	FOO_I8_ONE_HEX = 0x1,
}

enum EnumU8 : u8 {
	FOO_U8_POS = 255,
	FOO_U8_ZERO = 0,
	FOO_U8_POS_HEX = 0xfe,
	FOO_U8_ONE_HEX = 0x1,
}
}