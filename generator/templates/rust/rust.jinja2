{% macro render_namespaces(root) %}
{% for module in root.children if module | is_namespace %}

pub mod {{module.name}} {
    {{render_namespaces(module)-}}
    {% for node in module.children %}
        {%- if node | is_structure and not node | is_index %}
            {{- structure.declaration(node) }}
        {%- elif node | is_structure and node | is_index %}
            {{- index.declaration(node) }}
        {%- elif node | is_enumeration %}
            {{- enumeration.declaration(node) }}
        {%- elif node | is_archive %}
            {{- archive.declaration(node) }}
